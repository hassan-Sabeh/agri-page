<link rel="stylesheet" href="/styles/login.css">


<div id="form-login">
  
  <form class="form" method="POST" action="/login">
   <h1>Login</h1>
     <label>
      Email Adress:
      <input class="input email" type="email" placeholder="Email" id="input-email" name="email"/>
    </label>



     <label>
      Password:
      <input class="input password" type="password" name="Password" placeholder="password" id="input-password"/>
    </label>


    <input class="login" type="submit" value="Login"/>

   </form>
</div>

